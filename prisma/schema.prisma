generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RSVP {
  id        String   @id @default(cuid())
  name      String
  willAttend Boolean  // true = hadir, false = tidak hadir
  message   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("rsvps")
}
